@using DataAccess.DTO
@model List<DataAccess.Models.PostCategory>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@foreach (var post in Model)
{
    <p>@post.Category.Title</p>
    <p>@post.Post.Title</p>
}